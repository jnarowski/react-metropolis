{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "engines": {"node": "0.12.6"},
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js"
